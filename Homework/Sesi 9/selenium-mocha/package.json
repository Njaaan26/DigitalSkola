{
    "name": "selenium-mocha",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha",
    "scripts": {
        "test-coba": "mocha test.js --timeout 60000",
        "test-report": "mocha test.js --timeout 60000 --reporter mochawesome",
        "test-reportsecond": "mocha test.js --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=report.html"
    },
    "dependencies": {
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "mocha": "^10.4.0",
        "mochawesome": "^7.1.3",
        "mocha-simple-html-reporter": "^2.0.0"
    }
}
