{
    "name": "selenium-mocha-sesi11",
    "version": "1.0.0",
    "description": "Selenium webdriver test using mocha with Mochawesome report",
    "scripts": {
        "test": "mocha test/spec --recursive --timeout 60000 --reporter mochawesome",
        "login": "mocha test/spec/login.test.js --timeout 60000",
        "screenshot": "mocha test/spec/login.ss.test.js --timeout 60000",
        "visual": "mocha test/spec/visual.test.js --timeout 60000"
    },
    "dependencies": {
        "selenium-webdriver": "^4.17.0",
        "pixelmatch": "7.1.0",
        "pngjs": "7.0.0"
    },
    "devDependencies": {
        "mocha": "^10.4.0",
        "mochawesome": "^7.1.3",
        "mocha-simple-html-reporter": "2.0.0"
    }
}